{ mkDerivation, aeson, base, doctest, Glob, http-api-data, mtl
, stdenv, text
}:
mkDerivation {
  pname = "envelope";
  version = "0.2.2.0";
  sha256 = "deadbeef";
  isSimpleBuild = true;
  libraryHaskellDepends = [ aeson base http-api-data mtl text ];
